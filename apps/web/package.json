{
  "name": "@moxmuse/web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:run": "vitest run",
    "test:comprehensive": "vitest --config vitest.config.comprehensive.ts",
    "test:comprehensive:run": "vitest run --config vitest.config.comprehensive.ts",
    "test:unit": "vitest run --config vitest.config.comprehensive.ts src/**/*.test.{ts,tsx} packages/**/*.test.{ts,tsx}",
    "test:integration": "vitest run --config vitest.config.comprehensive.ts src/**/*integration*.test.{ts,tsx}",
    "test:performance": "vitest run --config vitest.config.comprehensive.ts src/**/*performance*.test.{ts,tsx}",
    "test:accessibility": "vitest run --config vitest.config.comprehensive.ts src/**/*accessibility*.test.{ts,tsx}",
    "test:e2e": "playwright test e2e/",
    "test:e2e:ui": "playwright test --ui e2e/",
    "test:coverage": "vitest run --coverage --config vitest.config.comprehensive.ts",
    "test:watch": "vitest --config vitest.config.comprehensive.ts",
    "test:ci": "npm run test:comprehensive:run && npm run test:e2e",
    "test:runner": "node -r tsx/register src/__tests__/test-runner.ts"
  },
  "dependencies": {
    "@moxmuse/api": "workspace:*",
    "@moxmuse/db": "workspace:*",
    "@moxmuse/shared": "workspace:*",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.1.5",
    "@tanstack/react-query": "^4.36.1",
    "@trpc/client": "^10.45.0",
    "@trpc/next": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@trpc/server": "^10.45.0",
    "@vercel/analytics": "^1.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "framer-motion": "^12.23.11",
    "lucide-react": "^0.303.0",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "superjson": "^2.2.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.10.5",
    "@types/papaparse": "^5.3.16",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.7.0",
    "@vitest/coverage-v8": "^3.2.4",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.0.4",
    "jest-axe": "^8.0.0",
    "jsdom": "^26.1.0",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^3.2.4"
  }
}