{
  "buildCommand": "pnpm --filter=@moxmuse/web run build",
  "installCommand": "pnpm install --filter=@moxmuse/web... --frozen-lockfile --config.dedupe-peer-dependents=false",
  "framework": null,
  "outputDirectory": "apps/web/.next",
  "ignoreCommand": "git diff HEAD^ HEAD --quiet .",
  "env": {
    "PNPM_VERSION": "8.14.0",
    "NODE_OPTIONS": "--max-old-space-size=4096",
    "VERCEL_BUILD_TIMEOUT": "45m"
  }
}
